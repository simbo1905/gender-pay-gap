@{
    ViewBag.Title = "Registration cancelled - Gender pay gap reporting service";
}
@model GenderPayGap.WebUI.Models.Register.OrganisationViewModel
<main id="content" role="main">
    <div class="grid-row">
        <div class="column-two-thirds">
            <div class="govuk-box-highlight">
                <h1 class="bold-large dynamic-content publicManual privateManual" style="display: block;">Registration request cancelled</h1>
            </div>
            <div class="bold-small">
                What happens next.
            </div>
            <p class="dynamic-content privateManual" style="display: block;">We’ve sent @Model.ContactFirstName&nbsp;@Model.ContactLastName an email, confirming their registration request for @Model.Name has been cancelled. </p>
        </div>
    </div>
</main>
