        <value>^[A-Za-z0-9]{6}$</value>
      </setting>
      <setting name="PinRegexError" serializeAs="String">
        <value>PIN code must contain 6 alpha or numeric characters</value>
      </setting>
      <setting name="EmployerPageSize" serializeAs="String">
        <value>10</value>
      </setting>
