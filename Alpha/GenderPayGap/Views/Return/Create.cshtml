                        <fieldset>
                            <div class="form-group">
                                @Html.LabelFor(model => model.MaleMiddlePayBand, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.MaleMiddlePayBand, new { htmlAttributes = new { @class = "form-control form-control-1-4" } }) %
                                    @Html.ValidationMessageFor(model => model.MaleMiddlePayBand, "", new { @class = "text-danger" })
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.FemaleMiddlePayBand, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.FemaleMiddlePayBand, new { htmlAttributes = new { @class = "form-control form-control-1-4" } }) %
                                    @Html.ValidationMessageFor(model => model.FemaleMiddlePayBand, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </fieldset>
                        <h3 class="heading-small">Upper middle quartile</h3>
                        <fieldset>
                            <div class="form-group">
                                @Html.LabelFor(model => model.MaleUpperPayBand, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.MaleUpperPayBand, new { htmlAttributes = new { @class = "form-control form-control-1-4" } }) %
                                    @Html.ValidationMessageFor(model => model.MaleUpperPayBand, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(model => model.FemaleUpperPayBand, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.FemaleUpperPayBand, new { htmlAttributes = new { @class = "form-control form-control-1-4" } }) %
                                    @Html.ValidationMessageFor(model => model.FemaleUpperPayBand, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </fieldset>
                        <h3 class="heading-small">Top quartile </h3>
                        <fieldset>
                            <div class="form-group">
                                @Html.LabelFor(model => model.MaleUpperQuartilePayBand, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.MaleUpperQuartilePayBand, new { htmlAttributes = new { @class = "form-control form-control-1-4" } }) %
                                    @Html.ValidationMessageFor(model => model.MaleUpperQuartilePayBand, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(model => model.FemaleUpperQuartilePayBand, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.FemaleUpperQuartilePayBand, new { htmlAttributes = new { @class = "form-control form-control-1-4" } }) %
                                    @Html.ValidationMessageFor(model => model.FemaleUpperQuartilePayBand, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <details>
                                <summary>What to include when calculating data by quartile</summary>
                                <div class="panel panel-border-narrow">
                                    <p>
                                    <p>Pay quartile reporting only requires your organisation to include ‘full-pay relevant employees’. This means every employee who, on the ‘snapshot date’, is both:</p>
                                    <ul class="list list-bullet">
                                        <li>employed </li>
                                        <li>being paid their full regular pay </li>
                                    </ul>
                                    <p>Don’t include:</p>
                                    <ul class="list list-bullet">
                                        <li>employees not paid in full at their normal rate for the ‘snapshot date’ because they are on leave (eg maternity, paternity or sick leave) </li>
                                    </ul>
                                    <a href="#">More guidance on full-pay relevant employees for pay quartile calculation  </a>
                                    </p>
                                </div>
                            </details>
                        </fieldset>
                        <h2 class="heading-medium">7. Link to your gender pay gap information</h2>
                        @*<p>Enter the web address (URL) for the gender pay gap information published on your organisation’s website. The person with responsibility for gender pay gap reporting in your organisation will have this.</p>*@
